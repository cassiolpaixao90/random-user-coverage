{
  "name": "random-user",
  "version": "1.0.0",
  "description": "Random user app, example for code coverage",
  "main": "dist/index.js",
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --content-base src/ --port 8080 --colors --progress --watch",
    "build": "NODE_ENV=production webpack --progress --colors && cp src/index.html dist/",
    "test": "NODE_ENV=test COVERAGE=true karma start"
  },
